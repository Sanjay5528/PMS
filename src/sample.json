{
	"pageHeading": "Bug Detail View",
	"formType": "simple",
	"editMode": "popup",
	"keyField": "_id",
	"form": {
	"collectionName": "bug",
	"fields": [
	
	{
		"fieldGroupClassName": "display-flex",
		"fieldGroup": [
			{
			"type": "html-input",
			"key": "testcase.test_data",
			"className": "flex-6",
			"props": {
				"label": "Test Case Test Data"
			}
		},
		{
		"type": "html-input",
		"key": "testcase.expected_result",
		"className": "flex-6",
		"props": {
			"label": "Test Case Expected Result"
		}
	}
]
},{
	"fieldGroupClassName": "display-flex",
	"fieldGroup": [
		{
		"type": "html-input",
		"key": "test_result.actual_result",
		"className": "flex-6",
		"props": {
			"label": "Test Case Actual Result"
		
		}
	},
	{
	"type": "muti-image",
	"key": "test_result.result_proof",
	"className": "flex-6",
	"props": {
		"label": "Result Proof Data"
	}
}
]
},{
	"fieldGroupClassName": "display-flex",
	"fieldGroup": [
		{
			"key": "devops_status",
			"type": "select",
			"templateOptions": {
				"label": "Devops Response",
				"options": [
					{
						"label": "Active",
						"value": "A"
					},
					{
						"label": "Disable",
						"value": "D"
					}
				],
				"required": true
			}
		},
		{
			"type": "html-input",
			"key": "test_result.actual_result",
			"className": "flex-6",
			"props": {
				"label": "Test Case Actual Result"
			
			}
		}
]
},
			{
				"key": "status",
				"type": "select",
				"defaultValue": "A",
				"templateOptions": {
					"label": "Status",
					"options": [
						{
							"label": "Active",
							"value": "A"
						},
						{
							"label": "Disable",
							"value": "D"
						}
					],
					"required": true
				},
				"hideExpression": "!model.isEdit"
			}
	
	],
		"defaultValues": [
			{
				"colName": "status",
				"value": "A",
				"type": "string"
			}
		]
	},
	"onCancelRoute": "list/bug"
}